#include <xil_io.h>

#define GPIO_ADDRESS	0x40000000
#define TIMER_ADDRESS	0x41C00000

#define TCSR0	0x00
#define TLR0	0x04
#define TCR0	0x08
#define TCSR1	0x10
#define TLR1	0x14
#define TCR1	0x18

#define C_PER	77

int set_pwm(UINTPTR Addr, u32 period, u32 impulse){
	Xil_Out32(GPIO_ADDRESS,	impulse);

}


int main() {
	Xil_Out32(GPIO_ADDRESS,	impulse);
}
